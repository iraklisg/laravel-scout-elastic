{
    "name": "irakisg/laravel-scout-elastic",
    "description": "Elastic Driver for Laravel Scout",
    "keywords": ["laravel", "scout", "elasticsearch", "elastic"],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/scout": "^5.0",
        "elasticsearch/elasticsearch": "^5.0",
        "illuminate/support": "^5.7"
    },
     "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~6.3",
        "illuminate/database": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Apptree\\Elasticsearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apptree\\Elasticsearch\\Test\\": "tests"
        }
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^5.0)."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Apptree\\Elasticsearch\\ElasticsearchProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
